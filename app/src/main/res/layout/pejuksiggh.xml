<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#5753E6B3"
    tools:context="com.tohellik.magerik.bom.tohellik.magerik.Kmjyysf">

    <Button
        android:id="@+id/bookivms"
        android:layout_width="373dp"
        android:layout_height="59dp"
        android:layout_marginBottom="32dp"
        android:onClick="golikemi"
        android:text="go Chat"
        android:textSize="30sp"
        app:backgroundTint="#A830DFDF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/qmskvudd"
        android:layout_width="204dp"
        android:layout_height="44dp"
        android:layout_marginTop="68dp"
        android:background="#3E804848"
        android:text="Chat Now?"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.55"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/iekjnssd"
        android:layout_width="234dp"
        android:layout_height="80dp"
        android:layout_marginTop="100dp"
        android:textColor="#416CEC"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.437"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/qmskvudd" />

    <ProgressBar
        android:id="@+id/kmchsaa"
        style="@android:style/Widget.Material.Light.ProgressBar.Small.Title"
        android:layout_width="182dp"
        android:layout_height="93dp"
        android:layout_marginTop="64dp"
        app:layout_constraintBottom_toTopOf="@+id/bookivms"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iekjnssd"
        app:layout_constraintVertical_bias="0.251" />

</androidx.constraintlayout.widget.ConstraintLayout>